{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "sourceMap": true,
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["cypress/**/*.ts"],
  "module": "commonjs",
  "moduleResolution": "node",
  "preserveValueImports": false,
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
}
